<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">
    <persistence-unit name="spaceship-unit">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>ru.orfac.lab2_extra.model.SpaceShip</class>
        <properties>
            <property name="javax.persistence.schema-generation.database.action"
                      value="drop-and-create"/>
            <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="javax.persistence.jdbc.url"
                      value="jdbc:postgresql://localhost:5432/orfac"/>
            <property name="javax.persistence.jdbc.user" value="orfac"/>
            <property name="javax.persistence.jdbc.password" value="orfac"/>
<!--            <property name="javax.persistence.jdbc.url"-->
<!--                      value="jdbc:postgresql://pg:5432/studs"/>-->
